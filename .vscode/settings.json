{
    "mocha.env": {
        "DOTENV_CONFIG_PATH": "env/test.env"
    },
    "mocha.files.glob": "test/*.js",
    "coverage-gutters.showGutterCoverage": false,
    "coverage-gutters.showLineCoverage": true,
    "coverage-gutters.showRulerCoverage": true,
    "coverage-gutters.partialHighlightLight": "rgba(220, 213, 143, 0.25)",
    "coverage-gutters.partialHighlightDark": "rgba(121, 86, 10, 0.25)",
    "coverage-gutters.noHighlightLight": "rgba(220, 143, 143, 0.25)",
    "coverage-gutters.noHighlightDark": "rgba(121, 31, 10, 0.25)",
    "coverage-gutters.highlightlight": "rgba(166, 220, 142, 0.25)",
    "coverage-gutters.highlightdark": "rgba(45, 121, 10, 0.25)",
    "mocha.sideBarOptions": {
        "lens": true,
        "decoration": true,
        "autoUpdateTime": 3600000,
        "showDebugTestStatus": true
    },
    "mocha.node_options": [
        "-r", "dotenv/config"
    ],
    "mocha.parallelTests": 8,
    // "lcov.sourceMaps": true,
    "lcov.watch": [],
    "lcov.branchCoverage": "simple",
    "cSpell.words": [
        "fhir",
        "genrsa",
        "nightwatch",
        "openid",
        "outform",
        "pubout",
        "replacestream"
    ],
    // "mocha.logVerbose": true
}